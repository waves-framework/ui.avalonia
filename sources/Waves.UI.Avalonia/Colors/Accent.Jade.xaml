<Style xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <Style.Resources>
        
        <system:String x:Key="ColorSetId">66280E94-94B0-4179-AA40-79B85E09DF63</system:String>
        <system:String x:Key="ColorSetName">Jade</system:String>

        <Color x:Key="Accent-Color-100">#A3DFCD</Color>
        <Color x:Key="Accent-Color-200">#7AD1B8</Color>
        <Color x:Key="Accent-Color-300">#3DBB97</Color>
        <Color x:Key="Accent-Color-400">#1FB187</Color>
        <Color x:Key="Accent-Color-500">#00A676</Color>
        <Color x:Key="Accent-Color-600">#009D71</Color>
        <Color x:Key="Accent-Color-700">#01946D</Color>
        <Color x:Key="Accent-Color-800">#028B68</Color>
        <Color x:Key="Accent-Color-900">#03705B</Color>

        <Color x:Key="Accent-Color-Foreground-100">#FFFFFF</Color>
        <Color x:Key="Accent-Color-Foreground-500">#E1E2E5</Color>
        <Color x:Key="Accent-Color-Foreground-900">#0B132B</Color>

        <!-- Accent -->
        <SolidColorBrush x:Key="Accent-100-Brush" Color="{StaticResource Accent-Color-100}" />
        <SolidColorBrush x:Key="Accent-200-Brush" Color="{StaticResource Accent-Color-200}" />
        <SolidColorBrush x:Key="Accent-300-Brush" Color="{StaticResource Accent-Color-300}" />
        <SolidColorBrush x:Key="Accent-400-Brush" Color="{StaticResource Accent-Color-400}" />
        <SolidColorBrush x:Key="Accent-500-Brush" Color="{StaticResource Accent-Color-500}" />
        <SolidColorBrush x:Key="Accent-600-Brush" Color="{StaticResource Accent-Color-600}" />
        <SolidColorBrush x:Key="Accent-700-Brush" Color="{StaticResource Accent-Color-700}" />
        <SolidColorBrush x:Key="Accent-800-Brush" Color="{StaticResource Accent-Color-800}" />
        <SolidColorBrush x:Key="Accent-900-Brush" Color="{StaticResource Accent-Color-900}" />

        <SolidColorBrush x:Key="Accent-Foreground-100-Brush" Color="{StaticResource Accent-Color-Foreground-100}" />
        <SolidColorBrush x:Key="Accent-Foreground-500-Brush" Color="{StaticResource Accent-Color-Foreground-500}" />
        <SolidColorBrush x:Key="Accent-Foreground-900-Brush" Color="{StaticResource Accent-Color-Foreground-900}" />
        
    </Style.Resources>

</Style>